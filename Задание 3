public class Main {
    public static void main(String[] args) {
        printColor(101);
    }

    static void printColor(int value) {

        if (value <= 0) {
            System.out.println("Красный");
        } else if (value >= 0 && value <= 100){
            System.out.println("Желтый");
        } else if (value > 100) {
            System.out.println("Зеленый");
        }
    }
}
